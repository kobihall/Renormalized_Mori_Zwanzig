        RowBox[{"a_", ",", "b_"}], "]"}], ",", "x__"}], "]"}], ",", "m_"}], 
    "]"}], ":=", 
   RowBox[{"Times", "[", 
    RowBox[{
     RowBox[{"Rational", "[", 
      RowBox[{"a", ",", "b"}], "]"}], ",", 
     RowBox[{"ROMify", "[", 
      RowBox[{
       RowBox[{"Times", "[", "x", "]"}], ",", "m"}], "]"}]}], "]"}]}], " ", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{
    "If", " ", "the", " ", "term", " ", "has", " ", "a", " ", "scalar", " ", 
     "multiple"}], ",", " ", 
    RowBox[{
    "bring", " ", "it", " ", "outside", " ", "the", " ", "ROMify", " ", 
     "call"}]}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"If", " ", "there", " ", "is", " ", "a", " ", "t", " ", "term"}], 
    ",", " ", 
    RowBox[{
    "bring", " ", "it", " ", "outside", " ", "the", " ", "ROMify", " ", 
     "call"}], ",", " ", 
    RowBox[{
    "and", " ", "reduce", " ", "the", " ", "max", " ", "order", " ", 
     "allowed", " ", "by", " ", "ROMify", " ", 
