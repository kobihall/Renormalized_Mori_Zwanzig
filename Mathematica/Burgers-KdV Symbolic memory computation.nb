    RowBox[{
     RowBox[{"CExpand", "[", "x", "]"}], ",", 
     RowBox[{"CExpand", "[", 
      RowBox[{"Plus", "[", "w", "]"}], "]"}]}], "]"}]}], 
  RowBox[{"(*", 
   RowBox[{
   "apply", " ", "to", " ", "each", " ", "term", " ", "in", " ", "a", " ", 
    "sum"}], "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
